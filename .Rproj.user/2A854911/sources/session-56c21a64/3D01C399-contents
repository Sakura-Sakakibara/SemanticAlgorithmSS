bubble_sort <- function(arr) {
  if (!is.null(dim(arr))) {
    stop('Not a vector')
  }

  n <- length(arr)
  for (i in 1:(n - 1)) {
    for (j in 1:(n - i)) {
      if (arr[j] > arr[j + 1]) {
        # Swap elements arr[j] and arr[j + 1]
        temp <- arr[j]
        arr[j] <- arr[j + 1]
        arr[j + 1] <- temp
      }
    }
  }
  return(arr)
}
